import React from 'react';
import Links from './Links';

function About(props) {
  return (
    <div id="about">
      <h2>About Me</h2>
      {props.bio && props.bio.length > 0 && <p>{props.bio}</p>}
      <Links 
        github={props.github}      // Pass github prop to Links
        linkedin={props.linkedin}  // Pass linkedin prop to Links
      />
    </div>
  );
}

export default About;